{
  "name": "@skyrim-webui/runtime-sdk",
  "version": "1.0.0",
  "description": "Skyrim Platform runtime for Skyrim WebUI framework",
  "keywords": [
    "skyrim",
    "skyrim platform"
  ],
  "author": "Mrowr Purr <mrowr.purr@gmail.com>",
  "homepage": "https://github.com/mrowrpurr/WebUI",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrowrpurr/WebUI.git"
  },
  "scripts": {
    "build": "tsc",
    "webpack": "webpack",
    "watch": "tsc-watch --onSuccess \"yarn webpack\"",
    "dev": "yarn && set DEPLOY_PLUGIN=true && tsc-watch --onSuccess \"yarn webpack\""
  },
  "bugs": {
    "url": "https://github.com/mrowrpurr/WebUI/issues"
  },
  "dependencies": {
    "@skyrim-platform/skyrim-platform": "^2.3.1",
    "@skyrim-webui/types": "^1.0.0",
    "@skyrim-webui/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@skyrim-webui/development-dependencies": "^1.0.0",
    "@skyrim-webui/test-util": "^1.0.0"
  }
}
