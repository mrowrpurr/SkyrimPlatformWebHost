{
  "name": "webui-private-browserEnv",
  "version": "1.0.0",
  "description": "Implementation of WebViewsHost JavaScript frontend which hosts all WebViews for WebUI",
  "main": "index.js",
  "scripts": {
    "build:watch": "tsc-watch -p tsconfig.build.json --onSuccess \"npm run build:deploy\"",
    "test:watch": "jest --watch",
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "pack": "webpack",
    "deploy": "@powershell copy build/browserEnvironment.js ../../../WebUI/__WebUI__/",
    "build:deploy": "webpack && npm run deploy"
  },
  "author": "Mrowr Purr <mrowr.purr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/puppeteer": "^5.4.4",
    "jest": "^27.4.4",
    "puppeteer": "^13.0.0",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "tsc": "^2.0.3",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
